{
  "name": "backend",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:user-model": "ts-node src/scripts/test-user-model.ts",
    "test:auth": "ts-node src/scripts/test-auth-endpoints.ts",
    "test:rbac": "ts-node src/scripts/test-rbac.ts",
    "test:websocket": "ts-node src/scripts/test-websocket.ts",
    "test:validation": "ts-node src/scripts/test-validation.ts",
    "migrate": "ts-node src/scripts/migrate.ts",
    "migrate:up": "ts-node src/scripts/migrate.ts up",
    "migrate:down": "ts-node src/scripts/migrate.ts down",
    "migrate:status": "ts-node src/scripts/migrate.ts status",
    "migrate:reset": "ts-node src/scripts/migrate.ts reset"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "joi": "^17.9.2",
    "pg": "^8.11.3",
    "ws": "^8.13.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "ts-node-dev": "^2.0.0",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/morgan": "^1.9.4",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/pg": "^8.10.2",
    "@types/ws": "^8.5.5",
    "@types/uuid": "^9.0.2",
    "@types/jest": "^29.5.3",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.1"
  }
}
